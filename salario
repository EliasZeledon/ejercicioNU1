package com.mycompany.ejercicio1;

import java.util.Scanner;

public class Ejercicio1 {

    public static void main(String[] args) {
        
        Scanner entrada= new Scanner (System.in);
       
         String nombre;
         Scanner sc=new Scanner (System.in);
         String apellido;
        
         int horas;
         int precio;
         int salario;
         
         
         System.out.println("cual es su nombre");
         nombre=sc.nextLine();
         
         System.out.println("Y su apellido?");
         apellido=sc.nextLine();
         
         System.out.println("cuantas horas trabaja durante el mes");
         horas= entrada.nextInt();
         
         System.out.println("cual es el precio por las horas que usted trabaja");
         precio= entrada.nextInt();
         
         salario=horas/precio;
         
         System.out.println("su salario de sus horas trabajadas es de: "+" "+salario);
    }
}
